body
	background-color: #fafafa
.content
	position: fixed
	left: 1%
	top: 1%
	list-style: none
#logo
	float: center
	margin: 5px
	padding: 15px
#logobar
	position: absolute
	left: 50%
	top: 0%
	-webkit-transform: translate(-50%, 0%);
	-moz-transform: translate(-50%, 0%);
	-ms-transform: translate(-50%, 0%);
	-o-transform: translate(-50%, 0%);
	transform: translate(-50%, 0%);
	height: 100%
.name
	text-align: left
	font-family: 'Quicksand'
	font-size: 22
	font-weight: 400
	color: white
.WIP
	text-align: left
	font-family: 'Quicksand'
	font-size: 12
	font-weight: 300
	color: white
#topbar
	z-index: 50
	position: fixed
	height: 10%
	width: 100%
	left: 0
	right: 0
	top: 0
	background-color: rgba(33,33,33,.6)
	padding: 0px 0px 0px 0px
	box-shadow: 0 1px 2px 0px black
a:visited, a:hover, a:active, a
	text-decoration: none
	color: #BBDEFB
	opacity: 1
.contact
	position: fixed
	right: 5%
	top: 4.5%
	font-family: 'quicksand'
	font-size: 12
	font-weight: 400
#content
#dircontent
	margin-left: -10px
	display: block
	position: relative
	top: 10%
.post
	float: left
	width: 50%
#thumb
	width: 100%
	height: auto
